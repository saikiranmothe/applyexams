<%= simple_form_for(@exam_notification) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :last_date %>
    <%= f.input :exam_date %>
    <%= f.input :age_limit %>
    <%= f.input :company_name %>
    <%= f.input :education %>
    <%= f.input :user_id %>
    <%= f.input :category ,collection: [ "MBA", "10th", "INTER"] %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
