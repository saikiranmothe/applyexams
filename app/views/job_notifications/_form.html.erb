<%= simple_form_for(@job_notification) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :company_name %>
    <%= f.input :vacancies %>
    <%= f.input :last_date %>
    <%= f.input :exam_date %>
    <%= f.input :age_limit %>
    <%= f.input :education %>
    <%= f.input :user_id %>
    <%= f.input :category %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
